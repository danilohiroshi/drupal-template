<?php

/**
 * @file
 * Install, update and uninstall functions for the dhc module.
 */

/**
 * Implements hook_install().
 */
function dh_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function dh_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
